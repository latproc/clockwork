PERIODMONITOR MACHINE freq_in {
	OPTION startup_enabled false;
	OPTION filter 0;
	OPTION delay 10;
	OPTION VALUE 0;
	check WHEN SELF IS idle && TIMER>=delay;
	idle DEFAULT;
	ENTER check { VALUE := freq_in.VALUE; }
}

# Generic sampling. Any machine may be monitored at the server
SAMPLE_PROPERTIES_CHANNEL CHANNEL {
	OPTION host "localhost";
	OPTION port 10600;
	KEY "a0693d2bf6f463bf96f9d660b8e157fb";
	VERSION "0.1.0";
	MONITORS `.*`;
	IGNORES STATE_CHANGES;
	IGNORES `SYSTEM`;
	IGNORES `C_WarningLight`;
	IGNORES `O_WarningLight`;
	IGNORES `O_StatusIndicator`;
	IGNORES `C_StatusIndicator`;
	IGNORES `.*SlowUpdateParams`;
	PUBLISHER;
}

