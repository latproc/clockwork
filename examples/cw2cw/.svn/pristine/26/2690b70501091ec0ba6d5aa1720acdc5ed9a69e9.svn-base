COREGRABCOMMUNICATION CHANNEL {
	OPTION port 7910;
	UPDATES M_Question SCREENQUESTIONINTERFACE;
	UPDATES Obj COUNTERINTERFACE;
	UPDATES a STRESSPAIRINTERFACE;
	UPDATES b STRESSPAIRINTERFACE;
}

FLAGONINTERFACE INTERFACE {
	on INITIAL;
	off STATE;
}

FLAGOFFINTERFACE INTERFACE {
	off INITIAL;
	on STATE;
}

GUARDOFFINTERFACE INTERFACE {
	false INITIAL;
	true STATE;
}

GUARDONINTERFACE INTERFACE {
	true INITIAL;
	false STATE;
}

PANELSTARTMSGINTERFACE INTERFACE {
	PowerIsOff INITIAL;
	ESRReset STATE;
	idle STATE;
}

SCREENQUESTIONINTERFACE INTERFACE {
  unknown INITIAL;
  off STATE;
  on STATE;
  ask STATE;

  COMMAND Question;
  COMMAND No;
  COMMAND Yes;
  COMMAND Reset;
}

STRESSPAIRINTERFACE INTERFACE {
	OPTION VALUE;
	OPTION delay;
	OPTION exe;

	idle INITIAL;
	one STATE;
	two STATE;
	three STATE;
	four STATE;

	COMMAND count;
}

CHANNELMONITORINTERFACE INTERFACE{
    interlocked INITIAL;
    on STATE;
    off STATE;
}

COUNTERINTERFACE INTERFACE {
	OPTION Count;
	Idle INITIAL;
	Working STATE;
}
